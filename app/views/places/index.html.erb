<div class="booyah-box">
<h1>Places</h1>
</div>

<%= will_paginate @places, previous_label: h("<"), next_label: h(">") %>

<% @places.each do |place| %>
  <div class="booyah-box col-10 offset-1">
    <h1><%= place.name %></h1><br />
    <i><%= place.address %></i><br />
    <p><%= place.description %></p><br />
  </div>
  <% end %>

  



